{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "IRCTC Automate",
  "version": "1.0.0",
  "description": "IRCTC Automation Extensoin",
  "manifest_version": 3,
  "author": "rksbot3837@gmail.com",
  "action":{
      "default_popup": "index.html",
      "default_title": "IRCTC Automation Extensoin",
      "default_icon": "logo192.png"
  },
  "icons": {
    "16": "logo192.png",
    "32": "logo192.png",
    "48": "logo192.png",
    "128": "logo192.png"
  },
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": ["https://www.irctc.co.in/*", "http://www.irctc.co.in/*"],
      "js": ["lib.js", "utils.js"],
      "run_at": "document_start"
    },
    {
      "matches": ["https://www.irctc.co.in/*", "http://www.irctc.co.in/*"],
      "js": ["start.js"],
      "run_at": "document_end"
    }
  ]
}
